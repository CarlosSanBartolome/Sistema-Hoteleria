@model HotelBahia.BussinesLogic.Domain.AsignacionHabitacion

@{
    ViewData["Title"] = "Eliminar asignacion";
}

<h3>¿Seguro que quiere eliminar esto?</h3>
<div>
    <h4>Asignacion de Habitacion</h4>
    <hr />
    <dl class="dl-horizontal">
       <dt>
            @Html.DisplayNameFor(model => model.Empleado)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Empleado.UsuarioNombre)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Habitacion)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Habitacion.Numero)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="AsignacionHabitacionId" />
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        <a asp-action="Index">Regresar a la lista</a>
    </form>
</div>
